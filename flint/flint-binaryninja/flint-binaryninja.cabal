test-suite flint-binaryninja-test
  type: exitcode-stdio-1.0
  main-is: Main.hs
  other-modules:
      Flint.Cfg.PathSpec
      Flint.Cfg.StoreSpec
      Flint.QuerySpec
      Spec
      Paths_flint_binaryninja
  autogen-modules:
      Paths_flint_binaryninja
  hs-source-dirs:
      test
  default-extensions:
      AllowAmbiguousTypes
      DataKinds
      DeriveAnyClass
      DerivingStrategies
      DuplicateRecordFields
      FunctionalDependencies
      GADTs
      LambdaCase
      NoImplicitPrelude
      NoMonomorphismRestriction
      OverloadedLabels
      OverloadedStrings
      TypeFamilies
      ViewPatterns
  ghc-options: -Wall -threaded -rtsopts -with-rtsopts=-N
  build-tool-depends:
      hspec-discover:hspec-discover
  build-depends:
      base >=4.7 && <5
    , blaze
    , blaze-binaryninja
    , flint
    , hspec
    , unordered-containers
  default-language: GHC2021
